Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{"clear", " ", "variables"}], " ", "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"Clear", "[", 
    RowBox[{
    "k", ",", "Nv", ",", "n", ",", "vmax", ",", "\[CapitalDelta]v", ",", "v", 
     ",", "Fj", ",", "Gj", ",", "Etil", ",", "ket", ",", "ketbra", ",", 
     "\[Alpha]j", ",", "H"}], "]"}], "\[IndentingNewLine]", 
   "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{"\"\<Dirac\>\"", " ", "to", " ", "vector"}], " ", "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"ket", "[", 
     RowBox[{"x_", ",", "n_"}], "]"}], ":=", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"y", ",", "res", ",", "i", ",", "\[Eta]"}], "}"}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"res", "=", 
        RowBox[{"Table", "[", 
         RowBox[{"0", ",", 
          RowBox[{"{", 
           RowBox[{"i", ",", "0", ",", 
            RowBox[{
             RowBox[{"2", "^", "n"}], "-", "1"}]}], "}"}]}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"res", "[", 
         RowBox[{"[", 
          RowBox[{"x", "+", "1"}], "]"}], "]"}], "=", "1"}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"Return", "[", "res", "]"}]}]}], "\[IndentingNewLine]", 
     "]"}]}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"ketbra", "[", 
     RowBox[{"x_", ",", "y_"}], "]"}], ":=", 
    RowBox[{
     RowBox[{"Transpose", "[", 
      RowBox[{"{", "x", "}"}], "]"}], ".", 
     RowBox[{"{", "y", "}"}]}]}], "\[IndentingNewLine]", 
   "\[IndentingNewLine]", 
   RowBox[{"(*", " ", "parameters", " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"n", "=", "5"}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"k", "=", "0.4"}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Nv", "=", 
     RowBox[{"2", "^", "n"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"vmax", "=", "4.5"}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"tmax", "=", 
     RowBox[{"8", "\[Pi]"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"\[CapitalDelta]v", "=", 
     RowBox[{"2", " ", 
      RowBox[{"vmax", "/", 
       RowBox[{"(", 
        RowBox[{"Nv", "-", "1"}], ")"}]}]}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"v", "[", "j_", "]"}], ":=", " ", 
    RowBox[{
     RowBox[{"-", "vmax"}], "+", 
     RowBox[{"2", "vmax", " ", 
      RowBox[{
       RowBox[{"(", "j", ")"}], "/", 
       RowBox[{"(", 
        RowBox[{"Nv", "-", "1"}], ")"}]}]}]}]}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Fj", "[", "j_", "]"}], ":=", 
    RowBox[{
     FractionBox["1", 
      RowBox[{"\[Sqrt]", 
       RowBox[{"(", 
        RowBox[{"2", "\[Pi]"}], ")"}]}]], 
     RowBox[{"Exp", "[", 
      RowBox[{
       RowBox[{"-", 
        FractionBox["1", "2"]}], " ", 
       RowBox[{
        RowBox[{"v", "[", "j", "]"}], "^", "2"}]}], "]"}]}]}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Gj", "[", "j_", "]"}], ":=", 
    RowBox[{
     FractionBox["1", 
      RowBox[{"\[Sqrt]", 
       RowBox[{"(", 
        RowBox[{"2", "\[Pi]"}], ")"}]}]], 
     RowBox[{"Exp", "[", 
      RowBox[{
       RowBox[{"-", 
        FractionBox["1", "2"]}], " ", 
       RowBox[{
        RowBox[{"v", "[", "j", "]"}], "^", "2"}]}], "]"}]}]}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"\[Alpha]j", "[", "j_", "]"}], ":=", 
    RowBox[{"Sqrt", "[", 
     RowBox[{"\[CapitalDelta]v", " ", 
      RowBox[{"Gj", "[", "j", "]"}]}], "]"}]}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Etil", "=", 
     RowBox[{
      FractionBox["I", "k"], 
      RowBox[{"Sum", "[", 
       RowBox[{
        RowBox[{"Fj", "[", "i", "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "0", ",", 
          RowBox[{"Nv", "-", "1"}]}], "}"}]}], "]"}], "\[CapitalDelta]v"}]}], 
    ";"}], "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{"(*", " ", "Hamiltonian", " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"getH", "[", "n_", "]"}], ":=", 
     RowBox[{"Sum", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"v", "[", "j", "]"}], 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"k", " ", 
           RowBox[{"ketbra", "[", 
            RowBox[{
             RowBox[{"ket", "[", 
              RowBox[{"j", ",", 
               RowBox[{"n", "+", "1"}]}], "]"}], ",", 
             RowBox[{"ket", "[", 
              RowBox[{"j", ",", 
               RowBox[{"n", "+", "1"}]}], "]"}]}], "]"}]}], "+", 
          RowBox[{
           RowBox[{"\[Alpha]j", "[", "j", "]"}], 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"ketbra", "[", 
              RowBox[{
               RowBox[{"ket", "[", 
                RowBox[{"j", ",", 
                 RowBox[{"n", "+", "1"}]}], "]"}], ",", 
               RowBox[{"ket", "[", 
                RowBox[{
                 RowBox[{"2", "^", "n"}], ",", 
                 RowBox[{"n", "+", "1"}]}], "]"}]}], "]"}], "+", 
             RowBox[{"ketbra", "[", 
              RowBox[{
               RowBox[{"ket", "[", 
                RowBox[{
                 RowBox[{"2", "^", "n"}], ",", 
                 RowBox[{"n", "+", "1"}]}], "]"}], ",", 
               RowBox[{"ket", "[", 
                RowBox[{"j", ",", 
                 RowBox[{"n", "+", "1"}]}], "]"}]}], "]"}]}], ")"}]}]}], " ", 
         ")"}]}], ",", 
       RowBox[{"{", 
        RowBox[{"j", ",", "0", ",", 
         RowBox[{
          RowBox[{"2", "^", "n"}], "-", "1"}]}], "}"}]}], "]"}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"H", "=", 
     RowBox[{"getH", "[", "n", "]"}]}], ";"}], "\[IndentingNewLine]", 
   "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{"initial", " ", "state"}], " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"x0", "=", 
     RowBox[{"(", "  ", 
      RowBox[{
       RowBox[{"Sum", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"Fj", "[", "j", "]"}], " ", 
          RowBox[{"ket", "[", 
           RowBox[{"j", ",", 
            RowBox[{"n", "+", "1"}]}], "]"}]}], ",", 
         RowBox[{"{", 
          RowBox[{"j", ",", "0", ",", 
           RowBox[{"Nv", "-", "1"}]}], "}"}]}], "]"}], "+", 
       RowBox[{"Etil", " ", 
        RowBox[{"ket", "[", 
         RowBox[{"Nv", ",", 
          RowBox[{"n", "+", "1"}]}], "]"}]}]}], "  ", ")"}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"\[Eta]", "=", 
     RowBox[{"Sqrt", "[", 
      RowBox[{
       RowBox[{"Conjugate", "[", "x0", "]"}], ".", "x0"}], "]"}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"x0", "=", " ", 
     RowBox[{"x0", "/", "\[Eta]"}]}], ";"}], "\[IndentingNewLine]", 
   "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{"time", " ", "evolve"}], " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"tdim", "=", "300"}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"dt", " ", "=", " ", 
     RowBox[{"tmax", "/", "tdim"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"x", "=", "x0"}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Et", "=", 
     RowBox[{"{", "}"}]}], ";", 
    RowBox[{"tlst", "=", 
     RowBox[{"{", "}"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"U", "=", 
     RowBox[{"MatrixExp", "[", 
      RowBox[{
       RowBox[{"-", "I"}], " ", "H", " ", "dt"}], "]"}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"xnorm", "=", 
     RowBox[{"{", "}"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{"For", "[", 
    RowBox[{
     RowBox[{"t", "=", "0"}], ",", 
     RowBox[{"t", "\[LessEqual]", "tdim"}], ",", 
     RowBox[{"t", "++"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"x", "=", 
       RowBox[{"U", ".", "x"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{
        RowBox[{"x", "[", 
         RowBox[{"[", 
          RowBox[{
           RowBox[{"2", "^", "n"}], "+", "1"}], "]"}], "]"}], " ", "is", " ", 
        "the", " ", "position", " ", "of", " ", "the", " ", "electric", " ", 
        "field"}], " ", "*)"}], "\[IndentingNewLine]", 
      RowBox[{"AppendTo", "[", 
       RowBox[{"tlst", ",", 
        RowBox[{"t", " ", "dt"}]}], "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"AppendTo", "[", 
       RowBox[{"Et", ",", 
        RowBox[{"x", "[", 
         RowBox[{"[", 
          RowBox[{
           RowBox[{"2", "^", "n"}], "+", "1"}], "]"}], "]"}]}], "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{"check", " ", "normalization"}], "*)"}], "\[IndentingNewLine]", 
      RowBox[{"AppendTo", "[", 
       RowBox[{"xnorm", ",", 
        RowBox[{"Sqrt", "[", 
         RowBox[{
          RowBox[{"Conjugate", "[", "x", "]"}], ".", "x"}], "]"}]}], 
       "]"}]}]}], "\[IndentingNewLine]", "]"}], "\[IndentingNewLine]", 
   RowBox[{"(*", " ", "plot", " ", "*)"}], "\[IndentingNewLine]", 
